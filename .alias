alias ll="ls -l"
alias la="ls -la"
alias zathura="zathura --mode=fullscreen"
alias lo='git log --oneline'
alias hist="git log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
alias st='git status'
alias ru='trans en:ru -b'
alias en='trans ru:en -b'
alias clip='xclip -selection clipboard'
